<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/ak"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main_Page2">

    <EditText
        android:id="@+id/text"
        android:layout_width="327dp"
        android:layout_height="35dp"
        android:layout_alignRight="@+id/search1"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginEnd="84dp"
        android:layout_marginRight="83dp"
        android:background="@color/colorAccent"
        android:ems="10"
        android:hint="Search or Enter URL"
        tools:ignore="InvalidId"></EditText>

    <Button
        android:id="@+id/search1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"


        android:layout_marginTop="-9dp"
        android:layout_marginEnd="-2dp"
        android:text="Search"
        tools:layout_editor_absoluteX="323dp"
        tools:layout_editor_absoluteY="0dp">


    </Button>

    <ImageButton
        android:id="@+id/google"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"

        android:layout_marginStart="31dp"
        android:layout_marginTop="108dp"
        app:srcCompat="@drawable/google" />

    <ImageButton
        android:id="@+id/facebook"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="107dp"
        android:layout_marginEnd="149dp"
        app:srcCompat="@drawable/facebook" />

    <ImageButton
        android:id="@+id/youtube"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="116dp"
        android:layout_marginEnd="1dp"
        app:srcCompat="@drawable/youtube" />

    <ImageButton
        android:id="@+id/twitter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="46dp"
        android:layout_marginTop="245dp"
        app:srcCompat="@drawable/twitter" />

    <ImageButton
        android:id="@+id/pinterest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="234dp"
        android:layout_marginEnd="140dp"
        app:srcCompat="@drawable/pinterest" />

    <ImageButton
        android:id="@+id/instagram"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="241dp"
        android:layout_marginEnd="0dp"
        tools:srcCompat="@drawable/instagram" />


</RelativeLayout>